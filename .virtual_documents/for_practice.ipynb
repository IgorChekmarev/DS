


import numpy as np
import pandas as pd
import matplotlib.pyplot as plt





matrix1, matrix2 = np.indices((10, 10))
matrix = (matrix1 + matrix2)%2
plt.imshow(matrix)





t = np.linspace(0, 20 * np.pi, 5000)
def x(t, alpha, beta):
    ax = alpha * (np.cos(t) + (np.cos(beta * t))/beta)
    return ax
    
def y(t, alpha, beta):
    ay = alpha * (np.sin(t) - (np.sin(beta * t))/beta)
    return ay





plt.plot(x(t, 4, 1.1), y(t, 4, 1.1))
plt.xlabel("x")
plt.ylabel("y")
plt.show()





alpha = np.array([2, 4, 6, 8, 10])
beta = np.array([1.2, 1.8, 2, 5, 10])
for i in range(5):
    for j in range(5):
        plt.plot(x(t, alpha[i], beta[j]), y(t, alpha[i], beta[j]))
        plt.xlabel("x")
        plt.ylabel("y")
        plt.title(f'alpha = {alpha[i]}, beta = {beta[j]}')
        plt.show()





