def mse(y_true: np.ndarray, y_pred: np.ndarray) -> float:
    """
    Mean Squared Error (MSE).

    Параметры
    ----------
    y_true : np.ndarray of shape (n,)
        Истинные значения таргета.
    y_pred : np.ndarray of shape (n,)
        Предсказанные значения.

    Возвращает
    ----------
        Среднеквадратичная ошибка: float.
    """
    y_true = np.asarray(y_true, dtype=float)
    y_pred = np.asarray(y_pred, dtype=float)

    return print('Среднеквадратичная ошибка:', np.mean(y_true - y_pred)**2)


def mae(y_true: np.ndarray, y_pred: np.ndarray) -> float:
    """
    Mean Absolute Error (MAE).

    Параметры
    ----------
    y_true : np.ndarray of shape (n,)
        Истинные значения таргета.
    y_pred : np.ndarray of shape (n,)
        Предсказанные значения.

    Возвращает
    ----------
        Средняя абсолютная ошибка: float.
    """
    y_true = np.asarray(y_true, dtype=float)
    y_pred = np.asarray(y_pred, dtype=float)

    return print('Средняя абсолютная ошибка:', np.mean(np.abs(y_true - y_pred)))


def mape(y_true: np.ndarray, y_pred: np.ndarray, *, percent: bool = False) -> float:
    """
    Mean Absolute Percentage Error (MAPE).

    По умолчанию возвращает долю (как в `sklearn.metrics.mean_absolute_percentage_error`).
    Если `percent=True`, умножает результат на 100.

    Параметры
    ----------
    y_true : np.ndarray of shape (n,)
        Истинные значения таргета.
    y_pred : np.ndarray of shape (n,)
        Предсказанные значения.
    percent : bool, default=False
        Возвращать результат в процентах.

    Возвращает
    ----------
        MAPE в долях (или в процентах при `percent=True`): float.
    """
    y_true = np.asarray(y_true, dtype=float)
    y_pred = np.asarray(y_pred, dtype=float)

    return print('MAPE в долях (или в процентах при `percent=True`):', np.mean(np.abs((y_true - y_pred) / y_true)) * 100)


def r2(y_true: np.ndarray, y_pred: np.ndarray) -> float:
    """
    Коэффициент детерминации R^2 для моделей с интерсептом.

    Параметры
    ----------
    y_true : np.ndarray of shape (n,)
        Истинные значения таргета.
    y_pred : np.ndarray of shape (n,)
        Предсказанные значения.

    Возвращает
    ----------
        R^2: float.
    """
    y_true = np.asarray(y_true, dtype=float)
    y_pred = np.asarray(y_pred, dtype=float)
    res = np.sum((y_true - y_pred) ** 2)
    tot = np.sum((y_true - np.mean(y_true)) ** 2)
    
    return print('R^2:', 1 - (res /tot))




